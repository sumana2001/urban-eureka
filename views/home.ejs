<%- include("partials/header"); -%>
<h1>Hey!</h1>
<p><%= startingContent %></p>
<%posts.forEach(function(post){%>
    <h1><%= post.title%></h1>
<% var trimmedContent = post.content.substring(0, Math.min(100,post.content.length)); %>
    <p><%= trimmedContent %>...
    <a href="/posts/<%=post.title %>">Read more</a>
</p>
    <%});%>
<%- include("partials/footer"); -%>